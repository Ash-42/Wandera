nlike.png\";\n\nconst Main = () => {\n    const [mainPosts, setmainPosts] = useState([]);\n\n    const {state, dispatch} = useContext(UserContext);\n\n    useEffect(() => {\n        fetch('/viewallposts', {\n            headers: {\n                \"Authorization\":\"Bearer \" + localStorage.getItem(\"jwt\")\n            }\n        })\n        .then(res => res.json())\n        .then(data => {\n            setmainPosts(data.posts);\n        })\n     });\n\n    const Like = (post_id) => {\n        fetch('/like', {\n            method: \"put\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n                \"Authorization\": \"Bearer \" + localStorage.getItem(\"jwt\")\n            },\n            body: JSON.stringify({\n                post_id: post_id\n            })\n        })\n        .then(res => res.json())\n        .then(result => {\n            const newLike = mainPosts.map(item => {\n                if(item._id === result._id) {\n                    return result\n                } else {\n                    return item\n                }\n            })\n            setmainPosts(newLike)\n        })\n        .catch(err => {\n            console.log(err);\n        })\n    }\n\n    const Unlike = (post_id) => {\n        fetch('/unlike', {\n            method: \"put\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n                \"Authorization\": \"Bearer \" + localStorage.getItem(\"jwt\")\n            },\n            body: JSON.stringify({\n                post_id: post_id\n            })\n        })\n        .then(res => res.json())\n        .then(result => {\n            const newUnlike = mainPosts.map(item => {\n                if(item._id === result._id) {\n                    return result\n                } else {\n                    return item\n                }\n            })\n            setmainPosts(newUnlike)\n        })\n        .catch(err => {\n            console.log(err);\n        })\n    }\n\n    const Comment = (comment, post_id)=>{\n        fetch('/comment',{\n            method: \"put\",\n            headers: {\n                \"Content-Type\":\"application/json\",\n                \"Authorization\":\"Bearer \" + localStorage.getItem(\"jwt\")\n            },\n            body: JSON.stringify({\n                post_id,\n                comment\n            })\n        })\n        .then(res => res.json())\n        .then(result => {\n            console.log(result);\n            const newComment = mainPosts.map(item => {\n              if(item._id === result._id){\n                  return result\n              } else {\n                  return item\n              }\n           })\n          setmainPosts(newComment)\n        })\n        .catch(err => {\n            console.log(err)\n        })\n    }\n\n    return (\n        <div className=\"main-page\">\n            <Navbar />\n            {\n                /* Make a copy of the mainPosts posts array, \n                and reverse it to display newest posts first  */\n                mainPosts.slice(0).reverse().map(item => {\n                    return (\n                        <div className=\"main-container\" key={item._id}>\n                            <div className=\"main-post-container\">\n                                <div className=\"user-main-info\">\n                                { \n                                    (item.postedByUName === (state ? JSON.parse(state).username : console.log(\"State is null\"))) ?\n                                    <a href={`/account`}>\n                                        <p>{item.postedByUName}</p>\n                                    </a>\n                                    : \n                                    <a href={`/account/${item.postedByUName}`}>\n                                        <p>{item.postedByUName}</p>\n                                    </a>\n                                }\n                                </div>\n                                <img src={item.photo} alt=\"User\" className=\"user-main-image\" />\n                                <div className=\"main-icons\">\n                                    <img src={like_icon} alt=\"Like\" onClick={() => Like(item._id)} />\n                                    <img src={unlike_icon} alt=\"Unlike\" onClick={() => Unlike(item._id)}/>\n                                    <p>{item.likedBy.length} Likes</p>\n                                </div>\n                                <div className=\"main-post-content\">\n                                    <h3>{item.title}</h3>\n                                    <p>{item.body}</p>\n                                    <h4>Comments</h4>\n                                    <div className=\"comment-section\">\n                                        {\n                                            item.comments.map(comment => {\n                                                return(\n                                                <div className=\"comments\" key={comment._id}>\n                                                    <span className=\"comment-author\">\n                                                        {comment.authorName}\n                                                    </span><br/>\n                                                    <p>{comment.content}</p>\n                                                </div>\n                                                )\n                                            })\n                                        }\n                                    </div>\n                                    <form id=\"post-comment\">\n                                        \n                                        <input type=\"text\" id=\"comment_text\" placeholder=\"Comment on this story!\" />  \n                                        <input type=\"button\" value=\"Post comment\" onClick={(e) => {\n                                            e.preventDefault();\n                                            Comment(document.forms[0].comment_text.value, item._id)\n                                        }}/>\n                                    </form>\n                                </div>\n                            </div>\n                        </div>                    \n                    );\n                })\n            }\n        </div>\n    )\n};\n\nexport default Main;","/home/srihebbar/Desktop/Wandera/client/src/pages/Login.js",[],"/home/srihebbar/Desktop/Wandera/client/src/pages/Post.js",[],"/home/srihebbar/Desktop/Wandera/client/src/components/NavbarMain.js",[],"/home/srihebbar/Desktop/Wandera/client/src/components/Albums.js",[],"/home/srihebbar/Desktop/Wandera/client/src/components/About.js",["85"],"import React, {useContext} from 'react';\nimport {UserContext} from \"../App\";\nimport \"./About.css\";\nimport ProfilePic from \"../images/profilepic.jpg\";\n\nconst About = () => {\n    const {state, dispatch} = useContext(UserContext);\n\n    return (\n        <div className=\"about\">\n            <div className=\"about-container\">\n                <div className='user-image'>\n                    <img src={ProfilePic} alt=\"User\"/>\n                </div>\n                <div className='user-info'>\n                    <div className=\"Name\">{state ? JSON.parse(state).name : console.log(\"State is null\")}</div>\n                    <div className=\"Username\">{state ? JSON.parse(state).username : console.log(\"State is null\")}</div>\n                    <div className=\"user-stats\">\n                        <span>{state ? JSON.parse(state).followers.length : console.log(\"State is null\")} followers</span>\n                        <span>{state ? JSON.parse(state).following.length : console.log(\"State is null\")} following</span>\n                    </div> \n                </div>\n            </div>\n        </div>\n    )\n};\n\nexport defa